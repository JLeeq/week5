        </span><span><span class="hljs-keyword">await</span></span><span> supabaseClient.</span><span><span class="hljs-title function_">from</span></span><span>(</span><span><span class="hljs-string">"todos"</span></span><span>).</span><span><span class="hljs-title function_">insert</span></span><span>({ text, </span><span><span class="hljs-attr">completed</span></span><span>: </span><span><span class="hljs-literal">false</span></span><span>, </span><span><span class="hljs-attr">user_id</span></span><span>: user.</span><span><span class="hljs-property">id</span></span><span> });
        </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"todoInput"</span></span><span>).</span><span><span class="hljs-property">value</span></span><span> = </span><span><span class="hljs-string">""</span></span><span>;
        </span><span><span class="hljs-title function_">loadTodos</span></span><span>();
      }

      </span><span><span class="hljs-comment">// 할일 삭제</span></span><span>
      </span><span><span class="hljs-keyword">async</span></span><span> </span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">deleteTodo</span></span><span>(</span><span><span class="hljs-params">id</span></span><span>) {
        </span><span><span class="hljs-keyword">await</span></span><span> supabaseClient.</span><span><span class="hljs-title function_">from</span></span><span>(</span><span><span class="hljs-string">"todos"</span></span><span>).</span><span><span class="hljs-title function_">delete</span></span><span>().</span><span><span class="hljs-title function_">eq</span></span><span>(</span><span><span class="hljs-string">"id"</span></span><span>, id);
        </span><span><span class="hljs-title function_">loadTodos</span></span><span>();
      }

      </span><span><span class="hljs-comment">// 렌더링</span></span><span>
      </span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">renderTodos</span></span><span>(</span><span><span class="hljs-params">todos</span></span><span>) {
        </span><span><span class="hljs-keyword">const</span></span><span> list = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"todoList"</span></span><span>);
        list.</span><span><span class="hljs-property">innerHTML</span></span><span> = todos
          .</span><span><span class="hljs-title function_">map</span></span><span>(
            </span><span><span class="hljs-function">(<span class="hljs-params">t</span></span></span><span>) =&gt; <span class="hljs-string">`
          &lt;li&gt;
            &lt;input type="checkbox" </span></span><span><span class="hljs-subst">${t.completed ? <span class="hljs-string">"checked"</span></span></span><span> : </span><span><span class="hljs-string">""</span></span><span>} /&gt;
            &lt;span class="todo-text"&gt;</span><span><span class="hljs-subst">${t.text}</span></span><span>&lt;/span&gt;
            &lt;button class="delete-btn" onclick="deleteTodo(</span><span><span class="hljs-subst">${t.id}</span></span><span>)"&gt;×&lt;/button&gt;
          &lt;/li&gt;`
          )
          .</span><span><span class="hljs-title function_">join</span></span><span>(</span><span><span class="hljs-string">""</span></span><span>);
      }

      </span><span><span class="hljs-comment">// ✅ 이벤트 등록</span></span><span>
      </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"signupBtn"</span></span><span>).</span><span><span class="hljs-title function_">addEventListener</span></span><span>(</span><span><span class="hljs-string">"click"</span></span><span>, signUp);
      </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"loginBtn"</span></span><span>).</span><span><span class="hljs-title function_">addEventListener</span></span><span>(</span><span><span class="hljs-string">"click"</span></span><span>, signIn);
      </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"logoutBtn"</span></span><span>).</span><span><span class="hljs-title function_">addEventListener</span></span><span>(</span><span><span class="hljs-string">"click"</span></span><span>, signOut);
      </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"addBtn"</span></span><span>).</span><span><span class="hljs-title function_">addEventListener</span></span><span>(</span><span><span class="hljs-string">"click"</span></span><span>, addTodo);

      </span><span><span class="hljs-comment">// ✅ deleteTodo 전역 등록</span></span><span>
      </span><span><span class="hljs-variable language_">window</span></span><span>.</span><span><span class="hljs-property">deleteTodo</span></span><span> = deleteTodo;

      </span><span><span class="hljs-variable language_">console</span></span><span>.</span><span><span class="hljs-title function_">log</span></span><span>(</span><span><span class="hljs-string">"✅ Supabase Todo App Ready"</span></span><span>);
    });
  </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">script</span></span></span><span>&gt;
</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">body</span></span></span><span>&gt;

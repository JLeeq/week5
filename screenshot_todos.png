10.8276 8.50176 11.4982 9.17239 11.4984 9.99963C11.4984 10.827 10.8277 11.4985 10.0003 11.4987C9.17283 11.4987 8.50131 10.8271 8.50131 9.99963C8.50149 9.17228 9.17294 8.50159 10.0003 8.50159Z"></path></svg></button></div></div></div></div></article><article class="text-token-text-primary w-full focus:outline-none [--shadow-height:45px] has-data-writing-block:pointer-events-none has-data-writing-block:-mt-(--shadow-height) has-data-writing-block:pt-(--shadow-height) [&amp;:has([data-writing-block])&gt;*]:pointer-events-auto scroll-mt-(--header-height)" tabindex="-1" dir="auto" data-turn-id="f81af5fe-c144-45d8-8870-4165e2366ebd" data-testid="conversation-turn-31" data-scroll-anchor="false" data-turn="user" style="scroll-margin-top: -9580px;"><h5 class="sr-only">나의 말:</h5><div class="text-base my-auto mx-auto pt-12 [--thread-content-margin:--spacing(4)] thread-sm:[--thread-content-margin:--spacing(6)] thread-lg:[--thread-content-margin:--spacing(16)] px-(--thread-content-margin)"><div class="[--thread-content-max-width:40rem] thread-lg:[--thread-content-max-width:48rem] mx-auto max-w-(--thread-content-max-width) flex-1 group/turn-messages focus-visible:outline-hidden mb-1 relative flex w-full min-w-0 flex-col" tabindex="-1"><div class="flex max-w-full flex-col grow"><div data-message-author-role="user" data-message-id="f81af5fe-c144-45d8-8870-4165e2366ebd" dir="auto" class="min-h-8 text-message relative flex w-full flex-col items-end gap-2 text-start break-words whitespace-normal [.text-message+&amp;]:mt-1"><div class="flex w-full flex-col gap-1 empty:hidden items-end rtl:items-start"><div class="user-message-bubble-color relative rounded-[18px] px-4 py-1.5 data-[multiline]:py-3 max-w-[var(--user-chat-width,70%)]" data-multiline=""><div class="whitespace-pre-wrap">아래는 전체 코드야. &lt;!DOCTYPE html&gt;
&lt;html lang="ko"&gt;
&lt;head&gt;
  &lt;meta charset="UTF-8" /&gt;
  &lt;meta name="viewport" content="width=device-width, initial-scale=1.0" /&gt;
  &lt;title&gt;Week 4 Full Stack Todo App (React + Express + SQLite)&lt;/title&gt;
  &lt;!-- React, Babel, Tailwind --&gt;
  &lt;script src="https://unpkg.com/react@18/umd/react.development.js"&gt;&lt;/script&gt;
  &lt;script src="https://unpkg.com/react-dom@18/umd/react-dom.development.js"&gt;&lt;/script&gt;
  &lt;script src="https://unpkg.com/@babel/standalone/babel.min.js"&gt;&lt;/script&gt;
  &lt;script src="https://cdn.tailwindcss.com"&gt;&lt;/script&gt;
&lt;/head&gt;

&lt;body class="bg-gray-100 min-h-screen flex items-center justify-center p-4"&gt;
  &lt;div id="root" class="w-full max-w-lg"&gt;&lt;/div&gt;

  &lt;script type="text/babel"&gt;

    // API URL 설정
    const API_URL = "http://127.0.0.1:3002/api/todos";

    // ✅ 개별 할일 항목 컴포넌트
    function TodoItem({ todo, onToggle, onDelete }) {
      return (
        &lt;li className="flex justify-between items-center p-3 border rounded-lg bg-white shadow-sm"&gt;
          &lt;div className="flex items-center gap-3"&gt;
            {/* 체크박스 */}
            &lt;input
              type="checkbox"
              checked={todo.completed}
              onChange={() =&gt; onToggle(todo.id)}
              className="w-5 h-5 text-blue-500"
            /&gt;
            &lt;span
              className={
                todo.completed
                  ? "line-through text-gray-400"
                  : "text-gray-800"
              }
            &gt;
              {todo.text}
            &lt;/span&gt;
          &lt;/div&gt;
          &lt;button
            onClick={() =&gt; onDelete(todo.id)}
            className="text-red-500 hover:text-red-700 text-sm"
          &gt;
            삭제
          &lt;/button&gt;
        &lt;/li&gt;
      );
    }

    // ✅ 할일 목록 컴포넌트
    function TodoList({ todos, onToggle, onDelete }) {
      if (todos.length === 0) {
        return &lt;p className="text-center text-gray-400"&gt;할일이 없습니다.&lt;/p&gt;;
      }

      return (
        &lt;ul className="space-y-2"&gt;
          {todos.map((todo) =&gt; (
            &lt;TodoItem
              key={todo.id}
              todo={todo}
              onToggle={onToggle}
              onDelete={onDelete}
            /&gt;
          ))}
        &lt;/ul&gt;
      );
    }

    // ✅ 메인 앱 컴포넌트
    function TodoApp() {
      const [todos, setTodos] = React.useState([]);
      const [inputText, setInputText] = React.useState("");
      const [filter, setFilter] = React.useState("all");
      const [loading, setLoading] = React.useState(false);

      // ✅ 서버에서 Todo 목록 불러오기 (GET)
      React.useEffect(() =&gt; {
        const fetchTodos = async () =&gt; {
          setLoading(true);
          try {
            const response = await fetch(API_URL);
            const data = await response.json();
            if (data.success) {
              setTodos(data.data);
            }
          } catch (error) {
            console.error("조회 실패:", error);
            alert("서버에 연결할 수 없습니다. 백엔드 서버가 실행 중인지 확인해주세요.");
          } finally {
            setLoading(false);
          }
        };
        
        fetchTodos();
      }, []);
      
      // ✅ 새 Todo 추가 (POST)
      const addTodo = async () =&gt; {
        if (!inputText.trim()) return alert("할일을 입력하세요!");
        
        setLoading(true);
        try {
          const response = await fetch(API_URL, {
            method: "POST",
            headers: { "Content-Type": "application/json" },
            body: JSON.stringify({ text: inputText }),
          });
          const data = await response.json();
          if (data.success) {
            setTodos([data.data, ...todos]); // 새 항목 추가
            setInputText(""); // 입력창 초기화
          } else {
            alert(data.error || "추가에 실패했습니다.");
          }
        } catch (error) {
          console.error("추가 실패:", error);
          alert("서버에 연결할 수 없습니다.");
        } finally {
          setLoading(false);
        }
      };
      
      // ✅ 할일 완료/미완료 토글 (PUT)
      const toggleTodo = async (id) =&gt; {
        const todo = todos.find((t) =&gt; t.id === id);
        if (!todo) return;
        
        setLoading(true);
        try {
          const response = await fetch(<code>${API_URL}/${id}</code>, {
            method: "PUT",
            headers: { "Content-Type": "application/json" },
            body: JSON.stringify({ completed: !todo.completed }),
          });
          const data = await response.json();
          if (data.success) {
            setTodos(todos.map((t) =&gt; (t.id === id ? data.data : t)));
          } else {
            alert(data.error || "수정에 실패했습니다.");
          }
        } catch (error) {
          console.error("수정 실패:", error);
          alert("서버에 연결할 수 없습니다.");
        } finally {
          setLoading(false);
        }
      };

      // Enter 키로 추가
      const handleKeyPress = (e) =&gt; {
        if (e.key === "Enter") addTodo();
      };

      // ✅ 할일 삭제 (DELETE)
      const deleteTodo = async (id) =&gt; {
        if (!window.confirm("삭제하시겠습니까?")) return;
        
        setLoading(true);
        try {
          const response = await fetch(<code>${API_URL}/${id}</code>, { method: "DELETE" });
          if (response.status === 204) {
            setTodos(todos.filter((t) =&gt; t.id !== id));
          } else {
            const data = await response.json();
            alert(data.error || "삭제에 실패했습니다.");
          }
        } catch (error) {
          console.error("삭제 실패:", error);
          alert("서버에 연결할 수 없습니다.");
        } finally {
          setLoading(false);
        }
      };

      // ✅ 전체 삭제 (완료된 항목만)
      const clearTodos = async () =&gt; {
        if (!window.confirm("완료된 할일을 모두 삭제하시겠습니까?")) return;
        
        setLoading(true);
        try {
          const completedTodos = todos.filter(t =&gt; t.completed);
          for (const todo of completedTodos) {
            await fetch(<code>${API_URL}/${todo.id}</code>, { method: "DELETE" });
          }
          setTodos(todos.filter(t =&gt; !t.completed));
        } catch (error) {
          console.error("전체 삭제 실패:", error);
          alert("서버에 연결할 수 없습니다.");
        } finally {
          setLoading(false);
        }
      };

      // ✅ 필터링된 목록
      const filteredTodos = todos.filter((todo) =&gt; {
        if (filter === "completed") return todo.completed;
        if (filter === "active") return !todo.completed;
        return true;
      });

      // ✅ 통계 계산
      const total = todos.length;
      const done = todos.filter((t) =&gt; t.completed).length;
      const doing = total - done;

      // ✅ UI 렌더링
      return (
        &lt;div className="max-w-3xl mx-auto p-6 md:p-8"&gt;
          {/* 헤더 */}
          &lt;header className="text-center mb-8"&gt;
            &lt;h1 className="text-3xl md:text-4xl font-bold text-blue-500 mb-2"&gt;
              📝 Full Stack Todo App
            &lt;/h1&gt;
            &lt;p className="text-gray-500"&gt;
              React + Express + SQLite
            &lt;/p&gt;
            {loading &amp;&amp; (
              &lt;p className="text-sm text-blue-500 mt-2"&gt;로딩 중...&lt;/p&gt;
            )}
          &lt;/header&gt;

          {/* 입력 영역 */}
          &lt;div className="bg-white rounded-lg shadow-md p-6 mb-6"&gt;
            &lt;h2 className="text-xl font-semibold text-gray-700 mb-4"&gt;
              할일 추가하기
            &lt;/h2&gt;

            &lt;div className="flex flex-col md:flex-row gap-3"&gt;
              &lt;input
                type="text"
                placeholder="새로운 할일을 입력하세요..."
                value={inputText}
                onChange={(e) =&gt; setInputText(e.target.value)}
                onKeyPress={handleKeyPress}
                disabled={loading}
                className="flex-1 px-4 py-2 border border-gray-300 rounded-lg focus:ring-2 focus:ring-blue-500 disabled:bg-gray-100"
              /&gt;
              &lt;button
                onClick={addTodo}
                disabled={loading || !inputText.trim()}
                className="w-full md:w-auto bg-blue-500 hover:bg-blue-600 disabled:bg-gray-400 text-white px-6 py-2 rounded-lg font-medium transition-colors"
              &gt;
                추가
              &lt;/button&gt;
            &lt;/div&gt;
          &lt;/div&gt;

          {/* 필터 버튼 */}
          &lt;div className="flex justify-center gap-2 mb-4"&gt;
            {["all", "active", "completed"].map((f) =&gt; (
              &lt;button
                key={f}
                onClick={() =&gt; setFilter(f)}
                disabled={loading}
                className={<code>px-3 py-1 rounded-lg text-sm disabled:opacity-50 ${
</code>                  filter === f
                    ? "bg-blue-500 text-white"
                    : "bg-gray-200 hover:bg-gray-300"
                }<code>}
</code>              &gt;
                {f === "all"
                  ? "전체"
                  : f === "active"
                  ? "진행중"
                  : "완료"}
              &lt;/button&gt;
            ))}
          &lt;/div&gt;

          {/* 목록 */}
          &lt;div className="bg-white rounded-lg shadow-md p-6"&gt;
            &lt;h2 className="text-xl font-semibold text-gray-800 mb-4"&gt;
              할일 목록 ({filteredTodos.length}개)
            &lt;/h2&gt;
            &lt;TodoList
              todos={filteredTodos}
              onToggle={toggleTodo}
              onDelete={deleteTodo}
            /&gt;
            {todos.length &gt; 0 &amp;&amp; (
              &lt;div className="text-right mt-4"&gt;
                &lt;button
                  onClick={clearTodos}
                  disabled={loading || done === 0}
                  className="bg-red-500 hover:bg-red-600 disabled:bg-gray-400 text-white px-4 py-2 rounded-lg disabled:opacity-50"
                &gt;
                  완료된 항목 삭제 ({done}개)
                &lt;/button&gt;
              &lt;/div&gt;
            )}
          &lt;/div&gt;

          {/* 통계 */}
          &lt;div className="bg-white rounded-lg shadow-md p-6 mt-6"&gt;
            &lt;h3 className="text-lg font-semibold text-gray-800 mb-3"&gt;
              📊 진행 현황
            &lt;/h3&gt;
            &lt;div className="grid grid-cols-3 gap-4 text-center"&gt;
              {[
                { label: "전체", value: total, color: "text-blue-600" },
                { label: "완료", value: done, color: "text-green-600" },
                { label: "남은 일", value: doing, color: "text-orange-600" },
              ].map((item) =&gt; (
                &lt;div key={item.label}&gt;
                  &lt;div className={<code>text-2xl font-bold ${item.color}</code>}&gt;
                    {item.value}
                  &lt;/div&gt;
                  &lt;div className="text-sm text-gray-500"&gt;{item.label}&lt;/div&gt;
                &lt;/div&gt;
              ))}
            &lt;/div&gt;
          &lt;/div&gt;
        &lt;/div&gt;
      );
    }

    // ✅ 렌더링
    ReactDOM.createRoot(document.getElementById("root")).render(&lt;TodoApp /&gt;);
  &lt;/script&gt;
&lt;/body&gt;

  }
}

</span><span><span class="hljs-comment">// 로그아웃</span></span><span>
</span><span><span class="hljs-keyword">async</span></span><span> </span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">signOut</span></span><span>(</span><span><span class="hljs-params"></span></span><span>) {
  </span><span><span class="hljs-keyword">await</span></span><span> supabase.</span><span><span class="hljs-property">auth</span></span><span>.</span><span><span class="hljs-title function_">signOut</span></span><span>();
